{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "outDir": "dist",
    "sourceMap": true,
    "strict": true,
    "target": "ES2020",
    "lib": ["es2020", "esnext.asynciterable", "dom", "dom.iterable", "esnext"],
    "jsx": "preserve",
    "isolatedModules": true,
    "incremental": true,
    "noImplicitAny": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "resolveJsonModule": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["generated", "src", "typings", "./**/*.ts", "**/*.tsx", "next-env.d.ts", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
